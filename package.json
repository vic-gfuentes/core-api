{
  "name": "core-api",
  "version": "1.0.0",
  "description": "core express - prisma - pg api",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "tsup": {
    "entry": [
      "src/index.ts"
    ]
  },
  "exports": {
    "default": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch --onSuccess \"node dist/index.mjs\"",
    "start": "node dist/index.mjs",
    "build": "tsup src/index.ts --format esm --dts --minify --clean --sourcemap && tsc-alias -p tsconfig.json",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.16.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@prisma/client": "5.13.0",
    "@types/cors": "^2.8.17",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "zod": "^3.23.8"
  }
}
